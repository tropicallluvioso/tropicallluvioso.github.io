---
import MainLayout from "../layouts/MainLayout.astro"
import '../styles/main.css';
import '../styles/slideshow.css';
import data from "./db.json"



const stills = data.es.strategic_comm.stills;
const text = data.es.strategic_comm.text;

console.log(text)


let {title} = Astro.props; 

---

<MainLayout title="Tropical Lluvioso  ">
<div class="container-slides">
     
      {stills.map((still) =>  
      <img loading="lazy" class="slide" src={`https://ucarecdn.com/${still.image.id}/`} alt=""
      >)}
      </div>
       <p class="pb-data">{text}</p>
  
</MainLayout>


<script src="../components/auto-slideshow.js"></script>


    
